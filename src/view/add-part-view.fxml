<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.addPartController">
   <children>
      <Label layoutX="73.0" layoutY="64.0" text="Add Part" />
      <RadioButton fx:id="inHouseRadio" layoutX="212.0" layoutY="64.0" mnemonicParsing="false" onAction="#onActionAddPartInHouse" text="In-House">
         <toggleGroup>
            <ToggleGroup fx:id="PartToggle" />
         </toggleGroup></RadioButton>
      <RadioButton fx:id="OutsourcedRadio" layoutX="339.0" layoutY="64.0" mnemonicParsing="false" onAction="#onActionAddPartOutsourced" text="Outsourced" toggleGroup="$PartToggle" />
      <Label layoutX="91.0" layoutY="116.0" text="ID" />
      <Label layoutX="92.0" layoutY="155.0" text="Name" />
      <Label layoutX="83.0" layoutY="200.0" text="Inventory" />
      <Label layoutX="81.0" layoutY="236.0" text="Price/Cost" />
      <Label layoutX="86.0" layoutY="276.0" text="Max" />
      <Label fx:id="MachineIDorCompany" layoutX="86.0" layoutY="314.0" text="Machine ID" />
      <TextField fx:id="partId" editable="false" layoutX="173.0" layoutY="112.0" style="-fx-background-color: lightgrey; -fx-border-color: grey; -fx-border-radius: 5; -fx-background-radius: 5;" />
      <Label layoutX="339.0" layoutY="276.0" text="Min" />
      <Button layoutX="357.0" layoutY="349.0" mnemonicParsing="false" onAction="#partSaveButton" text="Save" />
      <Button layoutX="432.0" layoutY="349.0" mnemonicParsing="false" onAction="#partCancelButton" text="Cancel" />
      <TextField fx:id="partName" layoutX="174.0" layoutY="151.0" />
      <TextField fx:id="partInventory" layoutX="175.0" layoutY="196.0" />
      <TextField fx:id="partPrice" layoutX="176.0" layoutY="232.0" />
      <TextField fx:id="partMax" layoutX="177.0" layoutY="272.0" />
      <TextField fx:id="partLastField" layoutX="178.0" layoutY="310.0" />
      <TextField fx:id="partMin" layoutX="372.0" layoutY="272.0" />
   </children>
</AnchorPane>
